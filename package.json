{
  "name": "ethereumjs-connect",
  "version": "4.0.0",
  "description": "Basic Ethereum connection tasks",
  "author": "The Augur Developers <team@augur.net>",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "build": "babel ./node_modules/ethereumjs-tx --source-root ./node_modules/ethereumjs-tx -d ./node_modules/ethereumjs-tx && browserify ./exports.js > ./dist/ethereumjs-connect.js && browserify ./exports.js | ./node_modules/uglify-js/bin/uglifyjs > ./dist/ethereumjs-connect.min.js && ./node_modules/browserify/bin/cmd.js test/*.js > test/browser/bundle.js",
    "lint": "eslint src && eslint test/*.js",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ethereumjs/ethereumjs-connect.git"
  },
  "keywords": [
    "Ethereum",
    "network",
    "connection"
  ],
  "bugs": {
    "url": "https://github.com/ethereumjs/ethereumjs-connect/issues"
  },
  "homepage": "https://github.com/ethereumjs/ethereumjs-connect#readme",
  "dependencies": {
    "async": "1.5.2",
    "clone": "1.0.2",
    "ethrpc": "3.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "browserify": "^12.0.1",
    "bufferutil": "^3.0.0",
    "chai": "^3.4.1",
    "coveralls": "^2.11.6",
    "eslint": "^3.17.1",
    "ethereumjs-stub-rpc-server": "2.0.3",
    "istanbul": "^0.4.1",
    "mocha": "^3.2.0",
    "uglify-js": "^2.6.1",
    "utf-8-validate": "^3.0.1"
  }
}
